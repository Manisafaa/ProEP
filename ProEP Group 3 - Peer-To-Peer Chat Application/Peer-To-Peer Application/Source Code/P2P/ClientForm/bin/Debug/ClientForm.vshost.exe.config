<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="false" httpsGetEnabled="false" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <client />
        <bindings>
            <netPeerTcpBinding>
                <binding name="BindingUnsecure">
                    <resolver mode="Pnrp" />
                    <security mode="None" />
                </binding>
            </netPeerTcpBinding>
        </bindings>
    </system.serviceModel>
</configuration>