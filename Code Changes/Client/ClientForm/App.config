<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="false" httpsGetEnabled="false" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <client>
            <endpoint address="http://localhost:1305/Service1.svc" binding="wsDualHttpBinding"
                bindingConfiguration="WSDualHttpBinding_IChat" contract="ChatServer.IChat"
                name="WSDualHttpBinding_IChat" />
        </client>
        <bindings>
            <netPeerTcpBinding>
                <binding name="BindingUnsecure">
                    <resolver mode="Pnrp" />
                    <security mode="None" />
                </binding>
            </netPeerTcpBinding>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IChat">
                    <security mode="None" />
                </binding>
            </wsDualHttpBinding>
        </bindings>
    </system.serviceModel>
</configuration>