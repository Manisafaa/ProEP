<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="false" httpsGetEnabled="false" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <client>
            <endpoint address="http://192.168.0.2:1305/IISHostedChat/Service1.svc/chat"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IChat"
                contract="ChatServer.IChat" name="WSDualHttpBinding_IChat">
                <identity>
                    <servicePrincipalName value="host/MHS-MBP" />
                </identity>
            </endpoint>
        </client>
        <bindings>
            <netPeerTcpBinding>
                <binding name="BindingUnsecure">
                    <resolver mode="Pnrp" />
                    <security mode="None" />
                </binding>
            </netPeerTcpBinding>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IChat" />
            </wsDualHttpBinding>
        </bindings>
    </system.serviceModel>
</configuration>